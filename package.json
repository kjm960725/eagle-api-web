{
  "name": "@kjm960725/eagle-api",
  "version": "1.0.2",
  "description": "iCrew Cloud API client for Node.js and browsers",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "icrew",
    "eagle",
    "api",
    "client",
    "websocket",
    "typescript"
  ],
  "author": "kjm960725",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjm960725/eagle-api-web.git"
  },
  "homepage": "https://github.com/kjm960725/eagle-api-web#readme",
  "bugs": {
    "url": "https://github.com/kjm960725/eagle-api-web/issues"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "isomorphic-ws": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
